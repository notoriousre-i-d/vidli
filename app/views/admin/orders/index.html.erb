<h1>Order History</h1>

<table>
  <tr>
    <th>Created On</th>
    <th>User</th>
    <th>Total</th>
  </tr>
<% @orders.each do |o| %>
  <tr>
    <td><%= link_to o.created_at, [:admin, o] %></td>
    <td><%= link_to o.user.full_name, [:admin, o.user] %></td>
    <td><%= number_to_currency o.total %></td>
  </tr>
<% end %>
</table>